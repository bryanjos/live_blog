<div>
  <%= form_tag("/sign/up", method: :post) %>
    <div>
      <label>Username</label>
      <input name="user[username]" type="text" value="<%=Map.get(@user,"username")%>"/>
    </div>

    <div>
      <label>Password</label>
      <input name="user[password]" type="password" />
    </div>

    <div>
      <label>Email</label>
      <input name="user[email]" type="text" value="<%=Map.get(@user, "email")%>"/>
    </div>

    <button type="submit">Submit</button>
  </form>
</div>
